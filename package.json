{
  "name": "template-typescript",
  "version": "1.2.0",
  "description": "The template of TypeScript.",
  "main": "index.ts",
  "author": "Kawano Kenichi <kenichi.k.820@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "check:lint": "eslint --fix --cache --ext .tsx,.ts,.js ./src",
    "check:format": "prettier . --write",
    "check:type": "tsc --project tsconfig.json --pretty --noEmit",
    "check:all": "npm-run-all -s check:type check:format check:lint",
    "test": "jest --runInBand",
    "test:cov": "jest --runInBand --coverage=true",
    "test:watch": "jest --runInBand --watch"
  },
  "lint-staged": {
    "*.{tsx,ts,js}": [
      "bash -c 'yarn check:type'",
      "prettier --write",
      "eslint --cache --fix"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "uuid": "^8.3.2",
    "zod": "^3.14.2"
  },
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.21",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "dotenv-cli": "^5.0.0",
    "esbuild-register": "^3.3.2",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.0",
    "jest": "^27.5.1",
    "lint-staged": "^12.3.7",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.0",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^3.14.0",
    "typescript": "^4.6.2"
  }
}
